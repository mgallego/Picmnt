<!DOCTYPE html>
<html lang="en">
    <head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<title>Picmnt{% block title %}{% endblock %}</title>

	{% block meta %}
	
    {% endblock meta %}


      <link href="/favicon.ico" rel="shortcut icon">

      {% block stylesheet %}
	      <link href="{{ asset('bundles/sfmpicmnt/bootstrap/css/picmnt.css') }}" rel="stylesheet" media="screen">
	      <link href="{{ asset('bundles/sfmpicmnt/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet" media="screen">
		  
		  <!--[if lt IE 9]>
			  <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
			  <![endif]-->
		  
	{% endblock stylesheet %}
	  
	{% block javascript %}
	  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js"></script>
	  <script src="{{ asset('bundles/sfmpicmnt/libs/jquery.mobile.custom.min.js') }}"></script>
	  <script src="{{ asset('bundles/sfmpicmnt/bootstrap/js/bootstrap.min.js') }}"></script>
	  <script src="{{ asset('bundles/sfmpicmnt/js/picmnt.js') }}"></script>
	{% endblock javascript %}

	{% block scripts %}
	  <script>

	      document.createElement('header');
	      document.createElement('canvas');
	      document.createElement('nav');
	      document.createElement('section');
	      document.createElement('article');
	      document.createElement('aside');
	      document.createElement('footer');
	      document.createElement('hgroup');

	      {#$(document).bin
d('pagecreate', function() {#}
	          $('#explore-tooltip').live('tap', function() {
	      	      $('#explore-tooltip').children('ul').toggle();
	          })
	      {#})#}

	  </script>
        {% endblock scripts %}

    </head>

    <body>
      {% block header %}

      {% endblock header %}

	  <div class="container">
      {% block content %}
      
      {% endblock %}
	  </div>

      {% block footer %}
        {% include 'SFMPicmntBundle::footer.html.twig' %}
      {% endblock footer %}
    </body>

</html>
