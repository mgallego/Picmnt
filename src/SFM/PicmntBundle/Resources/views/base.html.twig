<!DOCTYPE html>
<html>
  <head>

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>Picmnt{% block title %}{% endblock %}</title>

    {% block stylesheet %}
    <link rel="stylesheet" href="http://twitter.github.com/bootstrap/assets/css/bootstrap-1.2.0.min.css">
    <link rel="stylesheet" href="{{ asset('bundles/sfmpicmnt/stylesheets/layout.css')}}">
    <link rel="stylesheet" href="{{ asset('bundles/sfmpicmnt/fancybox/jquery.fancybox-1.3.4.css')}}">
    <link rel="stylesheet" href="{{ asset('bundles/sfmpicmnt/jqueryUI/css/custom-theme/jquery-ui-1.8.13.custom.css') }}">
    {% endblock %}

    {% block javascript %}
    <script src="http://code.jquery.com/jquery-1.5.2.min.js"></script>
    <script src="http://twitter.github.com/bootstrap/assets/js/application.js"></script>
    <script src="http://twitter.github.com/bootstrap/assets/js/google-code-prettify/prettify.js"></script>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.5.0/jquery.min.js"></script>  
    <script src="{{ asset('bundles/sfmpicmnt/fancybox/jquery.fancybox-1.3.4.pack.js')}}"></script>
    <script src="{{ asset('bundles/sfmpicmnt/js/picmnt.js') }}"> </script>
    <script src="{{ asset('bundles/sfmpicmnt/jqueryUI/js/jquery-ui-1.8.13.custom.min.js') }}"></script>
    <script src="http://twitter.github.com/bootstrap/1.3.0/bootstrap-dropdown.js"></script>

    {% endblock javascript %}

    {% block scripts %}
    {% endblock scripts %}
    
  </head>
  <body>
    {% block header %}header here{% endblock %}
    {% block menu %}{% endblock %}
    {% block fos_user_content %}{% endblock fos_user_content %}
    {% block content %}content here{% endblock %}
    {% include 'SFMPicmntBundle::footer.html.twig' %}
  </body>
</html>
